{
    "type": "library",
    "name": "luoyy/pay-php-sdk",
    "homepage": "https://github.com/zishang520/pay-php-sdk",
    "description": "WxPay and AliPay development of SDK",
    "license": "MIT",
    "keywords": [
        "pay-php-sdk",
        "alipay",
        "wxpay"
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "Pay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Pay": " Pay\\Facades\\Pay"
            },
            "providers": [
                "Pay\\Providers\\PayServiceProvider"
            ]
        }
    }
}
